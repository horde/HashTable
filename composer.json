{
    "name": "horde/hashtable",
    "description": "Hash table client library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-11-04",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {},
    "suggest": {
        "horde/log": "^3 || dev-FRAMEWORK_6_0",
        "horde/memcache": "^3 || dev-FRAMEWORK_6_0",
        "horde/vfs": "^3 || dev-FRAMEWORK_6_0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-0": {
            "Horde_HashTable": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\HashTable\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}